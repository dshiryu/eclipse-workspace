public class Felder {
    
    public static String[] boomBang(int start, int end) {
    	int difference = end - start;
    	int compare = 0;
    	String[] sequence = new String[difference];
    	String[] equal = new String[2];
    	if (start == end) {
    		equal[0] = String.valueOf(start);
    		equal[1] = String.valueOf(end);
    	} else if (difference < 2) {
    		for (int i = 0; i < difference; i++) {
	    		compare = start + i;
	    		if (compare % 3 == 0 && compare % 5 == 0) {
	    			equal [i] = String.valueOf("BoomBang");
				} else if (compare % 5 == 0) {
					equal [i] = String.valueOf("Bang");
	    		} else if (compare % 3 == 0) {
	    			equal [i] = String.valueOf("Boom");
	    		} else {
	    			equal [i] = String.valueOf(i+1);
	    		}
	    	}
    		return equal;
    	} else {
	    	for (int i = 0; i < difference; i++) {
	    		compare = start + i;
	    		if (compare % 3 == 0 && compare % 5 == 0) {
	    			sequence [i] = String.valueOf("BoomBang");
				} else if (compare % 5 == 0) {
	    			sequence [i] = String.valueOf("Bang");
	    		} else if (compare % 3 == 0) {
	    			sequence [i] = String.valueOf("Boom");
	    		} else {
	    			sequence [i] = String.valueOf(i+1);
	    		}
	    	}
    	}
    	
    	
        return sequence;
    }

    public static void tausche(int[] arr, int i, int j) {
    }

    public static void alleTauschen(int[] arr) {
    }

    public static void vertauscheN(int[] arr) {
    }

    public static int klumpen(int[] nums) {
        return 0;
    }

    /////////////
    // VORGABE //
    /////////////
    
    private static void print(int[] arr) {
        for (int i=0; i<arr.length ; i++) {
            System.out.print(arr[i] + " ");
        }
        System.out.println();
    }

    private static void print(String[] arr) {
        for (int i=0; i<arr.length ; i++) {
            System.out.print(arr[i] + " ");
        }
        System.out.println();
    }
    
    public static void main(String[] args) {

        int[] c = {};
        int[] d = {8,9,10,11,-1};
        int[] f = {5,5,2,2,2,3};
        int[] g = {0, 0, 0, 2, 2, 1, 1, 1, 2, 1, 1, 2, 2};

        System.out.println("\nAufgabe 1");
        print(boomBang(1,16));
        print(boomBang(15,15));
        print(boomBang(30,31));

        System.out.println("\nAufgabe 2");       
        tausche(d, 2, 3);
        print(d);
        alleTauschen(f);
        print(f);
        vertauscheN(g);
        print(g);

        System.out.println("\nAufgabe 3");       
        // Ergebnis: 5 2 0 0
        System.out.println(klumpen(g) + " " +klumpen(f) + " " +
                           klumpen(c) + " " + klumpen(d));

    }


}
