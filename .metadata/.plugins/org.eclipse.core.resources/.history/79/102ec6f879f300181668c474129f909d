
public class Main {

	public static void main(String[] args) {
		
		Dreieck abc = new Dreieck();
		
		abc.dreieck(7, 100, 7);
		abc.checkTriangle();
		
		abc.dreieck(10 , 10, 10);
		abc.checkTriangle();
		
		abc.dreieck(10000000, 50000, 3000);
		abc.checkTriangle();
		
		abc.dreieck(3, 4, 5);
		abc.checkTriangle();
		
		abc.dreieck(-1, 5, 10);
		abc.checkTriangle();
		
		
		
		Rechteck bla = new Rechteck();
		bla.rechteck(3, 4);
		bla.umfang();
		bla.flaeche();
		
		
	}

}
